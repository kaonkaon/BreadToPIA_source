text[10, 23] = "Ta det lugnt och samla så många poäng du kan.";
text[10, 24] = "Tidsattackläge";
text[10, 25] = "Tiden är begränsad! Gör ditt bästa för att samla poäng genom att slutföra order!";
text[10, 26] = "Till Huvudmenyn";
text[10, 27] = "Spela spelet, bli nummer ett!";
text[10, 28] = "Se hur man spelar spelet.";
text[10, 29] = "Byt språk, använd höger/vänster för att ändra";
text[10, 30] = "Högpoängdata läst: [OK]";
text[10, 31] = "Ansluter till databasen...";
text[10, 32] = "Kontrollera din anslutning";
text[10, 33] = "Databasanslutning [OK]";
text[10, 34] = "Se vem som hjälpte till att göra detta spel till verklighet";
text[10, 35] = "Tack för att du spelar detta spel!##Detta spel är baserat på (eller bara en remake av) det japanska virala spelet 'Suika Game'.##Målet med spelet är att kombinera 2 (eller ibland 3 om du har tur) lika bröd, utveckla till ett större bröd, samla poäng och bli nummer ett globalt.##Glöm inte ordern för att få bonuspoäng!";
text[10, 36] = "Flytta dina tänger med vänster eller höger knapp på din D-PAD, eller använd vänster analog pinne.#Släpp brödet med X-knappen.##Du kan också hålla brödet för att använda det senare genom att trycka på R-Trigger. Det hållna brödet kan bytas ut med samma knapp.##Du kan pausa genom att trycka på Start-knappen. Du kan också avsluta spelet från pausmenyn.";
text[10, 37] = "Brödutveckling:##Bolo -> Vada -> Maritozzo -> Boule -> Kanelbulle -> Pretzel -> Croissant -> Melonbröd -> Igelkottsbröd.##När 2 lika bröd rör varandra, kommer de att slås samman och utvecklas till ett större bröd.#Det kan också hända när 3 bröd rör varandra samtidigt. Men det händer sällan (det är en bugg, förresten).";
text[10, 38] = "Släpp inte brödet utanför behållaren!##Det tappade brödet exploderar och minskar din poäng med 300 poäng.";
text[10, 39] = "Spelet avslutas när brödet inte längre får plats i behållaren.";
text[10, 40] = "Det finns 2 spellägen!##Normalläge: Avslappnat spel, ingen tidsgräns.#Tidsattackläge: Tid är begränsad.##Samma mål: samla så många poäng som möjligt (och så snabbt som möjligt i tidsattackläge)";
text[10, 41] = "Speltillgångar:##Sprite: いらすとや (Irasuto-Ya) [www.irasutoya.com]#SFX: こうかおんラボ (SFX-Lab) [soundeffect-lab.info]#BGM: DOVA-SYNDROME [dova-s.jp]###Spelmotor, Byggare, Databas:##Spelmotor: GameMaker Studio 1.4.9999 av Yoyo Games [gamemaker.io]#VPK Byggare: GayMaker 1.7.7 av LiEnby [github.com/LiEnby/GayMaker]#SDK: LiteSDK av LiteSDK#SDK: PS Vita SDK av Sony#Byggare: Visual Studio Community 2015 av Microsoft#Web & Databas: WEB HOST MOST [webhostmost.com]###Huvudprogrammerare: Aqil Gama Rahmansyah 'Kaon' 'Ihvarear'##Special Thanks: My Family Member#### @ Jul-Aug, 2024";
text[10, 42] = "Den nya högpoängen kommer att laddas upp automatiskt till molnet!###*Den globala högpoängen kan återställas när som helst utan förvarning#*Du måste vara ansluten till internet för att använda den här funktionen.";

//Finnish
text[11, 0] = "Pelaa";
text[11, 1] = "Kuinka pelata";
text[11, 2] = "Krediitit";
text[11, 3] = "Valmis...";
text[11, 4] = "ALOITA!!!";
text[11, 5] = "Seuraava";
text[11, 6] = "Pidä";
text[11, 7] = "Pisteet";
text[11, 8] = "Paikallinen ennätys";
text[11, 9] = "Maailmanlaajuinen ennätys";
text[11, 10] = "Tilaa";
text[11, 11] = "Bonus";
text[11, 12] = "Oikea | Vasen: Liiku   X: Pudota   L Trigg: Pidä   Start: Tauko";
text[11, 13] = "Yritä uudelleen";
text[11, 14] = "Lopeta";
text[11, 15] = "Peli ohi...";
text[11, 16] = "Tauko";
text[11, 17] = "Takaisin";
text[11, 18] = "Oletko varma?";
text[11, 19] = "Ennätys tallennetaan automaattisesti";
text[11, 20] = "Kyllä";
text[11, 21] = "Ei";
text[11, 22] = "Normaali tila";
text[11, 23] = "Ota aikaa ja kerää niin paljon pisteitä kuin mahdollista.";
text[11, 24] = "Aikahyökkäystila";
text[11, 25] = "Aika on rajoitettu! Kerää mahdollisimman paljon pisteitä suorittamalla tilaukset!";
text[11, 26] = "Palaa päävalikkoon";
text[11, 27] = "Pelaa peliä, saavuta korkein piste!";
text[11, 28] = "Katso kuinka pelata peliä.";
text[11, 29] = "Vaihda kieltä, käytä oikeaa/vasenta vaihtaaksesi";
text[11, 30] = "Ennätysdata luettu: [OK]";
text[11, 31] = "Yhdistetään tietokantaan...";
text[11, 32] = "Tarkista yhteytesi";
text[11, 33] = "Tietokantayhteys [OK]";
text[11, 34] = "Katso, ketkä auttoivat tekemään tämän pelin todeksi";
text[11, 35] = "Kiitos, että pelaat tätä peliä!##Tämä peli perustuu (tai on vain remake) japanilaiseen viraalipeliin 'Suika Game'.##Pelin tavoite on yhdistää 2 (tai joskus 3, jos onni suosii) samaa leipää, kehittää isommaksi leiväksi, kerätä pisteitä ja tulla maailman ykköseksi.##Älä unohda tilausta saadaksesi bonusta!";
text[11, 36] = "Liiku pihdeilläsi painamalla D-PAD:in oikeaa tai vasenta nappia, tai käyttämällä oikeaa analogitikkua.#Pudota leipä painamalla X-nappia.##Voit myös pitää leivän myöhempää käyttöä varten painamalla R-Triggeä. Pidetty leipä voidaan vaihtaa samalla napilla.##Voit tauottaa painamalla Start-nappia. Voit myös lopettaa pelin taukovalikosta.";
text[11, 37] = "Leivän kehitys:##Bolo -> Vada -> Maritozzo -> Boule -> Kanelipulla -> Pretzel -> Croissant -> Melonileipä -> Siilileipä.##Kun 2 samaa leipää koskettavat toisiaan, ne yhdistyvät ja kehittyvät isommaksi leiväksi.#Se voi tapahtua myös, kun 3 leipää koskettavat toisiaan samanaikaisesti. Mutta se tapahtuu harvoin (se on muuten bugi).";
text[11, 38] = "Älä pudota leipää säiliön ulkopuolelle!##Pudonnut leipä räjähtää ja vähentää pisteitäsi 300 pisteellä.";
text[11, 39] = "Peli päättyy, kun leipä ei enää mahdu säiliöön.";
text[11, 40] = "Käytettävissä on 2 pelitilaa!##Normaali tila: Rentoa pelaamista, ei aikarajaa.#Aikahyökkäystila: Aika on rajoitettu.##Tavoitteena on kerätä mahdollisimman paljon pisteitä (ja mahdollisimman nopeasti aikahyökkäystilassa)";
text[11, 41] = "Peliresurssit:##Kuvat: いらすとや (Irasuto-Ya) [www.irasutoya.com]#Äänet: こうかおんラボ (SFX-Lab) [soundeffect-lab.info]#Musiikki: DOVA-SYNDROME [dova-s.jp]###Pelinmoottori, Rakentaja, Tietokanta:##Pelinmoottori: GameMaker Studio 1.4.9999 Yoyo Gamesilta [gamemaker.io]#VPK Rakentaja: GayMaker 1.7.7 LiEnbyltä [github.com/LiEnby/GayMaker]#SDK: LiteSDK by LiteSDK#SDK: PS Vita SDK by Sony#Rakentaja: Visual Studio Community 2015 Microsoftilta#Web & Tietokanta: WEB HOST MOST [webhostmost.com]###Pääohjelmoija: Aqil Gama Rahmansyah 'Kaon' 'Ihvarear'##Erityiskiitokset: Minun perheenjäsenet#### @ Heinä-Elokuu, 2024";
text[11, 42] = "Uusi ennätys ladataan automaattisesti pilveen!###*Maailmanlaajuinen ennätys voidaan nollata milloin tahansa ilman varoitusta#*Tämän ominaisuuden käyttämiseksi sinun on oltava yhteydessä internetiin.";

