text[9, 29] = "Ŝanĝi lingvon, premu dekstren/maldekstren por ŝanĝi";
text[9, 30] = "Altpoentaj datumoj legitaj : [OK]";
text[9, 31] = "Konektiĝanta al datumbazo...";
