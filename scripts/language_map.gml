// Lunari
text[14, 0] = "Yuko";
text[14, 1] = "Yuko Ankan";
text[14, 2] = "Krediton";
text[14, 3] = "Saja...";
text[14, 4] = "YUKO!!!";
text[14, 5] = "Nexan";
text[14, 6] = "Holto";
text[14, 7] = "Skorin";
text[14, 8] = "Lokalin Skorin";
text[14, 9] = "Globin Skorin";
text[14, 10] = "Ordorin";
text[14, 11] = "Bonusin";
text[14, 12] = "Raita | Levo: Movan   X: Dropon   L Trigg: Holto   Start: Pauso";
text[14, 13] = "Reku";
text[14, 14] = "Exito";
text[14, 15] = "Yuko Finito...";
text[14, 16] = "Pauso";
text[14, 17] = "Retron";
text[14, 18] = "Zure?";
text[14, 19] = "Skorin otomatik savin";
text[14, 20] = "Ja";
text[14, 21] = "Non";
text[14, 22] = "Normo Modo";
text[14, 23] = "Timo ezis, kompleton ordorin kun sen finin.";
text[14, 24] = "Timo Atako Modo";
text[14, 25] = "Timo limito! Faron vian bono por kompleton ordorin kaj gajni poenton!";
text[14, 26] = "Exiton al maina menu";
text[14, 27] = "Yuko la yuko, Gajni la alta skorin!";
text[14, 28] = "Vidi kiel yuko la yuko";
text[14, 29] = "Shanji lingvon, uzan raita/levo por shanji";
text[14, 30] = "Skorin daton read : [OK]";
text[14, 31] = "Konekton al databason...";
text[14, 32] = "Checki vian konekton";
text[14, 33] = "Databaso konekton [OK]";
text[14, 34] = "Vidi kiu helpis krean yuko";
text[14, 35] = "Gratiam por yukon!##Tiu yuko bazitas sur (au simpla refaro de) japana viral yuko 'Suika Yuko'.##La celo de tiu yuko estas kunigi 2 (au kelkfoje 3 pro bug) samaj panoj, evolui en pli granda pano, kolekti skorin, kaj fari la plej alta skorin globale.##Vi ankau povas kompleton ordorin por gajni bonus skorin!";
text[14, 36] = "Uzu la levo au raita butonoj sur via D-PAD por movi vian tonton, au uzu la raita analoga bastono.#Dropon la pano per premon X butono.##Vi povas holti la pano por uzon poste per premon R-Trigger. La pano kiu estas holton povas esti shanjita per uzon de sama butono.##Vi povas pauso per premon Start butono. Vi ankau povas fini la yuko de la pauso menu.";
text[14, 37] = "La evoluo de panoj estas:##Bolo -> Vada -> Maritozzo -> Boule -> Kanelon Buno -> Brezelo -> Krosanto -> Melono Pano -> Hizopano.##Kiam 2 samaj panoj tujas unu la alian, ili kunigi kaj evolui unu etapo.#Tio ankau povas okazi kiam 3 panoj tujas samtempe. Sed tio malofte okazas (pro bug).";
text[14, 38] = "Ne dropon vian pano ekster la ujo!##Dropon pano eksplodos kaj reduktos vian skorin per -300.";
text[14, 39] = "La yuko finos kiam panoj ne plu konvenas en la ujo.";
text[14, 40] = "2 yuko modoj estas haveblaj!##Normo Modo: Kazual ludo, akiri skorin kiel multe kiel vi povas dum ankau kompleton ordorin.#Timo Atako Modo: Tempo estas limigita! Akiri skorin kiel multe kiel vi povas, kiel rapide kiel vi povas! Kompleton ordorin por gajni bonus!";
text[14, 41] = "Yuko Assetoj:##Sprite: いらすとや (Irasuto-Ya) [www.irasutoya.com]#SFX: こうかおんラボ (SFX-Lab) [soundeffect-lab.info]#BGM: DOVA-SYNDROME [dova-s.jp]###Yuko Motoro, Builder, Databaso:##Yuko Motoro: GameMaker Studio 1.4.9999 by Yoyo Games [gamemaker.io]#VPK Builder: GayMaker 1.7.7 by LiEnby [github.com/LiEnby/GayMaker]#SDK: LiteSDK by LiteSDK#SDK: PS Vita SDK by Sony#Builder: Visual Studio Community 2015 by Microsoft#Web & Databaso: WEB HOST MOST [webhostmost.com]###Maina Programmero: Aqil Gama Rahmansyah 'Kaon' 'Ihvarear'##Specialo Danko: Mia Familio#### @ Jul-Aug, 2024";
text[14, 42] = "Nova skorin otomatik uploados al Nubo!###*Globala skorin povas esti reseti ajna tempo sen avizo#*Vi devas esti konektita al interreto por uzi tiun funkcion.";



